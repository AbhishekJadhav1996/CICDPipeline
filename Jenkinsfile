pipeline {
      agent any

  stages {

    stage("build") {
    steps {
        git 'https://github.com/AbhishekJadhav1996/CICDPipeline.git'  
        echo 'building the application..'
    }
  }

    stage("test") {
      steps {
              echo 'testing the application..'

      }
    }

    stage("deploy") {
      steps {
                echo 'deploying the application..'

      }
    }



  }
}
